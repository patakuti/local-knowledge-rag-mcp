{
  "embedding": {
    "provider": "ollama",
    "model": "nomic-embed-text",
    "dimensions": 768
  },
  "chunking": {
    "chunkSize": 1000,
    "chunkOverlap": 200
  },
  "search": {
    "minSimilarity": 0.7,
    "maxResults": 20,
    "maxChunks": 10
  },
  "indexing": {
    "include": ["**/*.md", "**/*.txt"],
    "exclude": ["**/node_modules/**", "**/.git/**"]
  }
}
